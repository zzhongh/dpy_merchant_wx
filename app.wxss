/**app.wxss**/
@import 'style/weui.wxss';
page,.page {
  height: 100%;
  font-family: 'PingFang SC', 'Helvetica Neue', Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.common-separator {
  background-color:#F2F2F2;
  height:1px;
  width: 100%
}

.common-zero-margin-top {
  margin-top: 0px
}

.common-five-separator {
  background-color:#E8F1FA;
  height:5px;
}

::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}

.common-arrow-right-image{
  width:15px;
  height:15px;
  position:absolute;
  right:10px
}


